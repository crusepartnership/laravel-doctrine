{
    "name": "atrauzzi/laravel-doctrine",
    "description": "An ORM for a Framework for Web Artisans",
    "keywords": ["doctrine", "laravel", "orm"],
    "license": "MIT",
    "authors": [{
        "name": "Alexander Trauzzi",
        "email": "atrauzzi@gmail.com"
    }],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*",
        "doctrine/orm": "2.5.*@dev",
        "doctrine/migrations": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.0",
        "illuminate/support": "4.*"
    },
    "autoload": {
        "psr-0": {
            "Atrauzzi\\LaravelDoctrine": "src/"
        }
    },

    "bin": ["bin/doctrine-laravel", "bin/doctrine-laravel.php"],

    "minimum-stability": "dev"
}